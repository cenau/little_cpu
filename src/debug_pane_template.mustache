
		<h2>{{header}}</h2>

		<div class = "registers">
			<p>PC:{{#wrapped}}{{mainCpu.pc}}{{/wrapped}}</p>

			{{#mainCpu.registers.general}}
				<p>REGISTER {{name}}:{{#wrapped}}{{value}}{{/wrapped}}</p>
			{{/mainCpu.registers.general}}
			{{#mainCpu.registers.equality}}
				<p>REGISTER {{name}}:{{#wrapped}}{{value}}{{/wrapped}}</p>
			{{/mainCpu.registers.equality}}

		</div>
		<div class = "memory">
			{{#mainCpu.memory}}
			<li contenteditable="true" class = "memcell">{{#wrapped}}{{.}}{{/wrapped}}</li>
			{{/mainCpu.memory}}
		</div>

